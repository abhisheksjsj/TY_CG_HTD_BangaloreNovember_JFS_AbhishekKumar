package com.tyss.thread.createThread;

public class TestC {
	public static void main(String[] args) {
		System.out.println("main started");
		PVR p = new PVR();
		User u1 = new User(p);
		u1.start();
		p.leaveMe();
		
		
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println("main ended...");
	}

}
