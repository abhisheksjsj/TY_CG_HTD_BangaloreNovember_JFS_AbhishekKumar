package com.capgemini.datastructure.sorting;

public class BubbleSort {
	static void bubbleSort(int[] ar) {
		int n = ar.length;
		int temp = 0;
		for (int i = 0; i < n; i++) {
			for (int j = 1; j < (n - i); j++) {
				if (ar[j - 1] > ar[j]) {
					// swap elements
					temp = ar[j - 1];
					ar[j - 1] = ar[j];
					ar[j] = temp;
				}

			}
		}
	}
}
